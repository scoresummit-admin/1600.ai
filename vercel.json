{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/openrouter/route",
      "destination": "/api/openrouter/route.py"
    },
    {
      "source": "/api/openai/responses",
      "destination": "/api/openai/responses.py"
    },
    {
      "source": "/api/py/exec",
      "destination": "/api/py/exec.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}